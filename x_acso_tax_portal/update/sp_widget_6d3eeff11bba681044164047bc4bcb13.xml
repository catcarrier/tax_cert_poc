	c.dateStartChange = function() {
		var newValue = c.data.dateStart.value;
		if(newValue == $scope.dateStartStash) return;
		$scope.dateStartStash = newValue;

		alert("newValue changed to " + $scope.dateStartStash);

		// TODO update data, emit events...
	}

	c.dateEndChange = function() {
		var newValue = c.data.dateEnd.value;
		if(newValue == $scope.dateEndStash) return;
		$scope.dateEndStash = newValue;

		alert(newValue);

		// TODO update data, emit events...
	}

}]]></client_script>
